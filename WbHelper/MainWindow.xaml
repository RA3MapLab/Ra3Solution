<Window x:Class="WbHelper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WbHelper"
        mc:Ignorable="d"
        Title="万能地编工具" Height="150" Width="400">
    <StackPanel>
        <Menu>
            <MenuItem Header="选项">
                <MenuItem Header="加入官方qq群" Click="JoinQQ"/>
                <MenuItem Header="更新链接" Click="UpdateLink"/>
            </MenuItem>
        </Menu>
        <Button Name="LaunchBtn" Width="200" Height="50" Margin="0,30,0,0" Click="LoadPlugin">启动</Button>
    </StackPanel>
</Window>
